{
  "expo": {
    "name": "ReceiptSplitter",
    "slug": "ReceiptSplitter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "exp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.receiptsplitter.app",
      "scheme": "exp"
    },
    "android": {
      "package": "org.receiptsplitter.app",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "exp",
              "host": "192.168.1.16",
              "pathPrefix": "/--/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b7e0aa76-fa14-45e4-b0a9-8dad3bcebafb"
      },
      "deepLinking": {
        "schemes": ["exp"]
      }
    },
    "owner": "addisonia"
  }
}